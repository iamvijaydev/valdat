{
  "name": "valdat",
  "version": "0.7.0",
  "description": "A modern day validator for the masses",
  "main": "dist/Valdat.js",
  "types": "dist/Valdat.d.ts",
  "scripts": {
    "setup": "npm install",
    "build": "tsc",
    "test": "jest",
    "clean": "shx rm -rf node_modules/ dist/ docs/",
    "typedoc": "typedoc --out ./docs --mode modules --tsconfig ./tsconfig.json ./lib/",
    "posttypedoc": "shx cp .nojekyll docs/.nojekyll"
  },
  "keywords": [
    "valdat",
    "valid",
    "validator",
    "validate",
    "validate-react"
  ],
  "author": "Vijay Dev <vijaydev.in@gmail.com> (http://vijaydev.com/)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iamvijaydev/valdat.git"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/lodash": "^4.14.109",
    "jest": "^23.1.0",
    "lodash": "^4.17.10",
    "shx": "^0.2.2",
    "ts-jest": "^22.4.6",
    "typedoc": "^0.11.1",
    "typescript": "^2.9.1"
  },
  "jest": {
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(ts?)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ]
  }
}
